infiles="feff.inp screenw.inp"
outfiles="beta.dat beta_0.dat"
git rm -rf ~/tmp/tmp_clean
mkdir ~/tmp/tmp_clean
cp $infiles ~/tmp/tmp_clean
cp $outfiles ~/tmp/tmp_clean
git rm -rf *
pd=`pwd`
cd ~/tmp/tmp_clean
cp $infiles $pd
for fl in $outfiles
do
	cp $fl $pd/$fl.reference
done
cd $pd
rm -r ~/tmp/tmp_clean
git add *
